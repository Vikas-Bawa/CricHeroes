clear errorMessage when receiving GET_TODOS_REQUEST\n    //[requestName]: requestState === \"FAILURE\" ? payload.message : \"\",\n    msg: requestState === \"FAILURE\" ? msg : \"\",\n  };\n};\n\nexport default errorReducer;\n","/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Redux/Actions/Auth.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Redux/Reducers/Api/LoadingReducer.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Redux/Actions/ActionStates.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Components/Core/PublicLayout.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Components/Core/PrivateLayout.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Components/Core/AppLayout.js",["170","171"],"import { useSelector } from \"react-redux\";\nimport { useNetworkStatus } from \"Hooks/NetworkStatus\";\n\nimport SelectTeam from \"Views/TeamComponent\";\n\n\n\n\n\n\nconst AppLayout = ({ isAuthenticated, children }) => {\n\n  const errorMsg = useSelector((state) => state.error.msg);\n  const networkStatus = useNetworkStatus();\n\n  return (\n    <>\n\n      <SelectTeam/>\n      error Msg:{errorMsg}\n      networkStatus:{networkStatus ? \"online \" : \"offline\"}\n      {children}\n    </>\n  );\n};\n\nexport default AppLayout;\n\n\n{/* error Msg: {errorMsg}\nnetwork status: {networkStatus ? \"online\" : \"offline\"} */}\n\n{/* {children} */}\n","/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Views/AboutUs.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Views/Login.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Views/SignUp.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Views/ForgotPassword.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Views/Dashboard.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Shared/RequestSaga.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Services/Api/Constants.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Services/Api/Auth.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Hooks/NetworkStatus.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Shared/Constants.js",[],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Views/TeamComponent/index.js",["172","173"],"/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Components/Cells/addTeamForm/index.js",["174","175","176"],"import CreateRadio from \"Components/Atoms/customRadio\";\nimport { InputField } from \"Components/Atoms/customInput\";\nimport React, { useState } from \"react\";\nimport Select from \"react-select\";\n\n\n\nfunction TeamForm({teamData,setTeamData,players,setPlayers,captain,setCaptain,allPlayers}) {\n\n \n  const [countryData,setCountryDate] = useState([]);\n\n   const handleInput = (data)=>{\n      setTeamData({...teamData,\n        [data.target.name] : data.target.value\n      })\n   }\n\n    const handleChange = (e) => {\n      console.log(e)\n          setCaptain(e)\n      };\n\n    \n    const handlePlayers=(players)=>{\n     \n     if(players.length<11){\n\n       setPlayers(players||[]);\n       setTeamData({...teamData,\n        teamPlayers:players\n      })\n    }\n    else if(players.length>10) {\n        return;\n    }\n      \n    }\n    const handleRadio=(event)=>{\n       console.log(\"event\" ,event)\n        setTeamData({...teamData,\n            teamType:event\n        })\n        \n    }\n    // function selectFunction(e){\n    // return e.playername;\n    // }\n\n    const filterOption = (option, inputValue) => {\n      const { label, value } = option;\n      // looking if other options with same label are matching inputValue\n      const otherKey = option.filter(\n        opt => opt.label === label && opt.value.includes(inputValue)\n      );\n      return value.includes(inputValue) || otherKey.length > 0;\n    };\n    \n  return (\n    <div>\n      <form>\n      <InputField\n            name=\"teamName\"\n            type=\"text\"\n            label=\"Team Name\"\n            placeholder=\"Enter the Team Name\"\n            value={teamData[\"teamName\"]}\n            onChange={handleInput}\n          />\n          <br />\n      <label><b>Select the type of team</b></label><br/>\n        <CreateRadio values={[\"ipl\",\"international\"]} name={\"Type of team\"} state={teamData.teamType} setState={handleRadio}/>\n        \n        <label><b>Select the Players</b></label>\n\n        <Select options={allPlayers.map(val=>({label:val.playername,value:val.playername}))} onChange={handlePlayers}   name=\"teamPlayers\" value={players} isMulti />\n        {players.length>9?<div className=\"text-danger\">11 players selected</div>:null}\n        <label><b>Select Captain for the team</b></label>\n        <Select options={players.map(val=>({label:val.label,value:val.value}))} onChange={handleChange}  name=\"teamCaptain\" value={captain} />\n      </form>\n    </div>\n  );\n}\n\nexport default TeamForm;\n","/home/chicmic/Documents/Cricket_Scheduling_App/CricketApp-1/src/Components/Atoms/customModal/index.js",["177"],"import React, { useState } from 'react'\nimport Button from 'react-bootstrap/Button';\nimport Modal from 'react-bootstrap/Modal';\n\nfunction CustomModal(props) {\n    const handleClose = () =>{\n        props.setShowModal(false);\n    }\n\n    const handleSubmit =()=>{\n        console.log('Modal submitted');\n        props.setShowModal(false);\n      }\n  \n    return (\n      <>\n\n        <Modal\n          show={props.showModal}\n          onHide={handleClose}\n          backdrop=\"static\"\n          keyboard={false}\n        >\n          <Modal.Header closeButton>\n            <Modal.Title>{props.title}</Modal.Title>\n          </Modal.Header>\n          <Modal.Body>\n            {props.children}\