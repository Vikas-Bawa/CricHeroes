derRoutes routes={PUBLIC_ROUTES} />\n        </PublicLayout>\n      </Route>\n      <Redirect from=\"*\" to={DEFAULT_GUEST_ROUTE} />\n    </Switch>\n  );\n};\n\nconst AuthenticatedRoutes = () => {  \n  return (\n    <Switch>\n        <Route exact path={PUBLIC_ROUTES.map((route) => route.path)}>\n        <PublicLayout>\n          <RenderRoutes routes={PUBLIC_ROUTES} />\n        </PublicLayout>\n      </Route>\n\n        <Route path={PRIVATE_ROUTES.map((route) => route.path)}>\n        <PrivateLayout>\n          <RenderRoutes routes={PRIVATE_ROUTES} />\n       </PrivateLayout>\n        </Route>\n     \n     <Redirect from=\"*\" to={DEFAULT_AUTHENTICATED_ROUTE} />\n    \n      </Switch>\n  );\n};\n\nconst RootRouter = () => {\n\n const dispatch=useDispatch()\n  useEffect(() => {\n    dispatch(getData([]));\n  }, [])\n\n  const token = useSelector((state) => state.data.token);\n\n  const baseName = process.env.REACT_APP_BASE_NAME;\n  const isAuthenticated = !!token;\n  return (\n\n\n    <BrowserRouter basename={baseName}>\n      <DocumentTitle isAuthenticated={isAuthenticated} />\n      <AppLayout isAuthenticated={isAuthenticated}>{token? <AuthenticatedRoutes /> : <GuestRoutes token={token}/>}</AppLayout>\n    </BrowserRouter>\n\n  );\n};\n\nexport default RootRouter;\n","/home/chicmic/CricketApp/src/Redux/loginStore.js",[],"/home/chicmic/CricketApp/src/Redux/Sagas/RootSaga.js",[],"/home/chicmic/CricketApp/src/Redux/Reducers/RootReducer.js",[],"/home/chicmic/CricketApp/src/Routes/AuthRoutes.js",[],"/home/chicmic/CricketApp/src/Routes/PrivateRoutes.js",["215"],"import SelectTeam from \"Views/SelectTeam\";\nimport ScheduleMatch from \"../Views/ScheduleMatch\";\nimport Logout from \"Views/Logout\";\nexport const PRIVATE_ROUTES = [\n  // {\n  //   path: \"/selectTeam\",\n  //   component: SelectTeam,\n  //   title: \"Select Team\",\n  // },\n  {\n    path: \"/scheduleMatch\",\n    component: ScheduleMatch,\n    title: \"Schedule Match\",\n  },\n  {\n    path: \"/logout\",\n