uthmessage\";\n\nconst GuestRoutes = ({token}) => {\n  return (\n    <Switch>\n      <Route exact path={AUTH_ROUTES.map((route) => route.path)}>\n        <RenderRoutes routes={AUTH_ROUTES} />\n      </Route>\n     \n      <Route exact path={PUBLIC_ROUTES.map((route) => route.path)}>\n        <PublicLayout>\n          <RenderRoutes routes={PUBLIC_ROUTES} />\n        </PublicLayout>\n      </Route>\n      <Redirect from=\"*\" to={DEFAULT_GUEST_ROUTE} />\n    </Switch>\n  );\n};\n\nconst AuthenticatedRoutes = () => {  \n  return (\n    <Switch>\n        <Route exact path={PUBLIC_ROUTES.map((route) => route.path)}>\n        <PublicLayout>\n          <RenderRoutes routes={PUBLIC_ROUTES} />\n        </PublicLayout>\n      </Route>\n\n        <Route path={PRIVATE_ROUTES.map((route) => route.path)}>\n        <PrivateLayout>\n          <RenderRoutes routes={PRIVATE_ROUTES} />\n       </PrivateLayout>\n        </Route>\n     \n     <Redirect from=\"*\" to={DEFAULT_AUTHENTICATED_ROUTE} />\n    \n      </Switch>\n  );\n};\n\nconst RootRouter = () => {\n\n const dispatch=useDispatch()\n  useEffect(() => {\n    dispatch(getData([]));\n  }, [])\n\n  const tokenData = useSelector((state) => state.loginReducer.token);\n  const token=tokenData.length\n  // console.log(tokenData[0])\n  const baseName = process.env.REACT_APP_BASE_NAME;\n  const isAuthenticated = !!token;\n  return (\n\n\n    <BrowserRouter basename={baseName}>\n      <DocumentTitle isAuthenticated={isAuthenticated} />\n      <AppLayout isAuthenticated={isAuthenticated}>{token? <AuthenticatedRoutes /> : <GuestRoutes token={token}/>}</AppLayout>\n    </BrowserRouter>\n\n  );\n};\n\nexport default RootRouter;\n","/home/chicmic/CricketApp/src/Redux/loginStore.js",[],"/home/chicmic/CricketApp/src/Redux/Sagas/RootSaga.js",[],["222","223"],"/home/chicmic/CricketApp/src/Redux/Reducers/RootReducer.js",[],"/home/chicmic/CricketApp/src/Routes/AuthRoutes.js",[],"/home/chicmic/CricketApp/src/Routes/PrivateRoutes.js",[],"/home/chicmic/CricketApp/src/Routes/PublicRoutes.js",["224"],"import ShowPlayer from \"../Views/ShowPlayer\";\nimport ShowTeam from \"../Views/ShowTeam\";\nimport Home from \"../Views/Home\";\nimport SelectTeam from \"Views/SelectTeam\";\n\n\nexport const PUBLIC_ROUTES = [\n  {\n    path: \"/\",\n    component: Home,\n    title: \"Homepage\",\n    exact: true,\n  },\n  {\n    path: \"/showPlayer\",\n    component: ShowPlayer,\n    title: \"Show Player\",\n  },\n  {\n    path: \"/showTeam\",\n    component: ShowTeam,\n    title: \"Show Team\",\n  },\n  \n  \n];\n","/home/chicmic/CricketApp/src/Routes/DocumentTitle.js",[],"/home/chicmic/CricketApp/src/Routes/RenderRoutes.js",[],"/home/chicmic/CricketApp/src/Redux/Reducers/RootReducer/loginRootReducer.js",[],"/home/chicmic/CricketApp/src/Redux/Sagas/Auth.js",[],"/home/chicmic/CricketApp/src/Redux/Reducers/Auth.js",[],"/home/chicmic/CricketApp/src/Redux/Reducers/Api/LoadingReducer.js",[],"/home/chicmic/CricketApp/src/Redux/Reducers/Api/ErrorReducer.js",[],"/home/chicmic/CricketApp/src/Redux/Actions/Auth.js",[],"/home/chicmic/CricketApp/src/Redux/Actions/ActionStates.js",[],"/home/chicmic/CricketApp/src/Components/Core/AppLayout.js",["225"],"import { useSelector } from \"react-redux\";\n\nimport Navbar from \"Components/Atoms/Navbar\";\n\nimport { useNetworkStatus } from \"Hooks/NetworkStatus\";\n\n\nconst AppLayout = ({ isAuthenticated, children }) => {\n  // const errorMsg = useSelector((state) => state.error.msg);\n  const networkStatus = useNetworkStatus();\n\n  return (\n    <>\n      <Navbar/>\n      {/* error Msg: {errorMsg} */}\n      <div className=\"d-flex justify-content-end w-100\">\n     {networkStatus ? <label className=\" m-2 px-4 py-2 text-success\">Online</label> : <label className=\" m-2 px-4 py-2  text-danger\">Offline</label>}\n      </div>\n      {children}\n    </>\n  );\n};\n\nexport default AppLayout;\n\n\n","/home/chicmic/CricketApp/src/Components/Core/PrivateLayout.js",[],"/home/chicmic/CricketApp/src/Components/Core/PublicLayout.js",[],"/home/chicmic/CricketApp/src/Redux/Sagas/loginSaga/index.js",["226"],"import axios from 'axios'\nimport { takeLatest, put, call, all } from 'redux-saga/effects'\nimport { setData, setTeamData } from 'Redux/Actions/loginActions'\nimport { ADDTEAM, GETDATA } from 'Redux/Actions/loginActions/actionStates';\nimport { UPDATETEAM } from 'Redux/Actions/updateTeamActions/actionStates';\n\nfunction* players(payload) {\n  try {\n    const response = yield axios.get(\"https://customcricketmatch-default-rtdb.firebaseio.com/Vplayers.json\");\n    const playersDataWithKey = []\n    for (let key in response.data) {\n      playersDataWithKey.push({ ...response.data[key], \"key\": key })\n    }\n    yield put(setData(playersDataWithKey));\n  }\n  catch (error) {\n    if (payload && payload?.fail) {\n      payload.fail(error)\n    }\n  }\n}\n\nfunction* teams(payload) {\n  try {\n    const response = yield axios.get(\"https://customcricketmatch-default-rtdb.firebaseio.com/Vteams.json\");\n    const teamsDataWithKey = []\n    for (let key in response.data) {\n      teamsDataWithKey.push({ ...response.data[key], \"key\": key })\n    }\n    yield put(setTeamData(teamsDataWithKey));\n  }\n  catch (error) {\n    if (payload && payload?.fail) {\n      payload.fail(error)\n    }\n  }\n}\n\nfunction* addTeam(payload) {\n  try {\n    yield axios.post(\"https://customcricketmatch-default-rtdb.firebaseio.com/Vteams.json\", payload.data);\n  }\n  catch (error) {\n    if (payload && payload?.fail) {\n      payload.fail(error)\n    }\n  }\n}\n\nfunction* updatePlayersTeam(payload) {\n  try {\n    const teamName = Object.keys(payload.data)[0];\n    const teamObj = {\n      label: teamName,\n      value: teamName,\n    }\n    let teamArr;\n    debugger;\n    const requests = payload.data[teamName].map((playerData) => {\n      teamArr = [...playerData.Team,{...teamObj} ];\n      \n      return axios.patch(`https://customcricketmatch-default-rtdb.firebaseio.com/Vplayers/${playerData.key}.json`,\n        { Team: teamArr },\n      )\n      \n    })\n    yield axios.all(requests)\n  }\n  catch (error) {\n    if (payload && payload?.fail) {\n      payload.fail(error)\n    }\n  }\n}\n\nfunction* Sagaa() {\n  yield all([takeLatest(GETDATA, players), takeLatest(GETDATA, teams), takeLatest(ADDTEAM, addTeam), takeLatest(UPDATETEAM, updatePlayersTeam)]);\n}\nexport default Sagaa;\n","/home/chicmic/CricketApp/src/Redux/Actions/loginActions/index.js",[],"/home/chicmic/CricketApp/src/Views/ScheduleMatch/index.js",["227"],"import React from 'react'\nimport { useSelector } from 'react-redux'\nfunction ScheduleMatch() {\n\n  return (\n    <>\n      <h2>Schedule Your Match Here</h2>\n    </>\n  )\n}\n\nexport default ScheduleMatch","/home/chicmic/CricketApp/src/Views/ShowTeam/index.js",["228"],"import React, { useEffect } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport CustomTable from \"Components/Cells/customTable\";\nimport { getData } from \"Redux/Actions/loginActions\";\n\nfunction ShowTeam() {\n  const  data = useSelector((state) => state.loginReducer.teams);\n  const dispatch = useDispatch();\n  useEffect(() => {\n    dispatch(getData([]));\n  }, [])\n\n  const teamHeadings = [\n    {\n      label: \"TeamName\",\n      key: \"teamName\",\n    },\n    {\n      label: \"TeamCaptain\",\n      key: \"teamCaptain\",\n    },\n    {\n      label: \"TeamType\",\n      key: \"teamType\",\n    },\n    {\n      label: \"TeamPlayers\",\n      key: \"teamPlayers\",\n    },\n  ];\n  return (\n    <>\n      <CustomTable tableContent={data} headingDetails={teamHeadings} btnText='View Players' component='teamName'/>\n    </>\n  );\n}\n\nexport default ShowTeam;","/home/chicmic/CricketApp/src/Views/ShowPlayer/index.js",["229"],"\nimport CustomTable from 'Components/Cells/customTable';\nimport { useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux'\nimport { getData } from 'Redux/Actions/loginActions';\n\n\n// const arr = [\"Player name\", \"Player PhoneNo\", \"Player Age\", \"JerseyyNo\", \"PlayerCountry\", \"sixes\", \"fours\", \"fiftys\", \"Hundreds\", \"Score\", \"Avg score\", \"inings played\"];\nconst playerTableHeading=[\n  {\n  label:\"Name\",\n  key:\"Name\"\n  },\n  {\n    label:\"Age\",\n    key:\"Age\"\n    },\n    {\n      label:\"JerseyNo\",\n      key:\"JerseyNo\"\n      },\n      {\n        label:\"PhoneNo\",\n        key:\"PhoneNo\"\n        },\n        {\n          label:\"Country\",\n          key:\"Country\"\n          },\n          {\n            label:\"Fours\",\n            key:\"Fours\"\n            },\n            {\n              label:\"Sixes\",\n              key:\"Sixes\"\n              },{\n                label:\"Half Centuries\",\n                key:\"Fifties\"\n\n              },\n              {\n                label:\"Centuries\",\n                key:\"Hundreds\"\n              },\n              {\n                label:\"Total Score\",\n                key:\"Score\"\n              },\n              {\n                label:\"Avg score\",\n                key:\"AvgScore\"\n              },\n              {\n                label:\"Innings\",\n                key:\"InningsPlayed\"\n              },\n              {\n                label:\"Team\",\n                key:\"Team\"\n              }\n]\nfunction Main() {\nconst data = useSelector((state) => state.loginReducer.players);\nconsole.log('.....show player dataa.....',data);\nconst dispatch = useDispatch()\nuseEffect(() => {\n  dispatch(getData([]));\n}, [])\n\nreturn (\n    <div>\n      <CustomTable tableContent={data} headingDetails={playerTableHeading} btnText='View Team'  component='Name'/>\n\n    </div>\n  )\n}\n\nexport default Main\n","/home/chicmic/CricketApp/src/Views/Home/index.js",[],"/home/chicmic/CricketApp/src/Redux/Reducers/loginReducer/index.js",["230"],"import { REHYDRATE } from \"redux-persist\";\n\n\nimport { SETDATA, GETDATA, Token, SETTEAMDATA} from \"Redux/Actions/loginActions/actionStates\";\n\nconst initialData = {\n  players: [],\n  teams: [],\n  token: null,\n}\n\n\nconst loginReducer = (data = initialData, action) => {\n  switch (action.type) {\n     case GETDATA:\n        return data\n    case SETDATA:\n      return { ...data, players: action.data.map((data) => ({ ...data })) };\n    case SETTEAMDATA:\n      return { ...data, teams: action.data.map((data) => ({ ...data })) };\n\n    case Token:\n      return { ...data, token: action.token}\n\n      // case REHYDRATE:\n      //   let persistedData = ((action || {}).payload).loginReducer || initalData\n      //   console.log(persistedData, action,data,\"persistedData<><<><><>\")\n      //   return {\n      //     ...data,\n      //     token:persistedData.token\n      //   }\n    default:\n      return data;\n  }\n}\n\nexport default loginReducer;","/home/chicmic/CricketApp/src/Shared/RequestSaga.js",[],"/home/chicmic/CricketApp/src/Views/SelectTeam/index.js",["231","232","233","234"],"/home/chicmic/CricketApp/src/Views/Login/index.js",["235"],"/home/chicmic/CricketApp/src/Views/Logout/index.js",[],"/home/chicmic/CricketApp/src/Services/Api/Auth.js",[],"/home/chicmic/CricketApp/src/Redux/Actions/loginActions/actionStates.js",[],"/home/chicmic/CricketApp/src/Hooks/NetworkStatus.js",[],"/home/chicmic/CricketApp/src/Components/Atoms/Carousel/index.js",[],"/home/chicmic/CricketApp/src/Components/Atoms/Navbar/index.js",[],"/home/chicmic/CricketApp/src/Services/Api/Constants.js",[],"/home/chicmic/CricketApp/src/Shared/Constants.js",[],"/home/chicmic/CricketApp/src/Components/Cells/customTable/index.js",["236"],"import React, { useState } from \"react\";\nimport Pagination from \"../../Atoms/Pagination\";\nimport TableButton from \"Components/Atoms/TableButton\";\n\n\nexport default function CustomTable({ tableContent, headingDetails, btnText, component }) {\n  const [showperpage, setShowperPage] = useState(11);\n  const [paginate, setPaginate] = useState({\n    start: 0,\n    end: showperpage\n  })\n  const onPaginationChange = (start, end) => {\n\n    setPaginate({\n      start: start,\n      end: end\n    })\n  }\n\n  return (\n    <>\n\n      <div className=\" justify-content-center w-100 px-5 pb-4\">\n        <table className=\"table  table-success table-bordered table-hover  table-sm \">\n          <thead className='table table-dark'>\n            <tr>\n              {headingDetails.map(val =>\n                <th>\n                  <div className='d-flex '>\n                    {val.label}\n                  </div>\n                </th>\n              )}\n            </tr>\n          </thead>\n          {tableContent.length ?\n            <tbody>\n\n              {tableContent.slice(paginate.start, paginate.end).map((val, index) =>\n                <tr>\n                  {headingDetails.map(heading => <td >{(typeof val[heading.key] !== 'object') ?\n                    val[heading.key] :\n                    <TableButton data={val[heading.key]} btnText={btnText} firstColumnValue={tableContent[index][component]} />\n                  }</td>)}\n                </tr>\n\n              )}\n            </tbody>\n            :\n            <div className=\"d-flex justify-content-center w-100 p-5\">\n              <h1 className=\" text-danger \">Loading Data Please Wait.....</h1>\n            </div>\n          }\n        </table>\n      </div>\n      {tableContent.length ? <Pagination showperPage={showperpage} onPaginationChanges={onPaginationChange} total={tableContent.length} /> : null}\n    </>\n  )\n}\n","/home/chicmic/CricketApp/src/Components/Atoms/Pagination/index.js",["237","238"],"import React, { useEffect, useState } from 'react'\n\nconst Pagination = ({showperPage,onPaginationChanges,total}) => {\n    const[counter,setCounter]=useState(1);\n    \n    const onButtonClick=(type)=>{\n        if(type==='prev'){\n            if(counter===1){\n                setCounter(1);\n            }\n            else {\n                \n                setCounter(counter-1);\n            }\n        }\n        else if(type==='next'){\n            if(Math.ceil(total/showperPage)===counter||total==0){\n                setCounter(counter);\n            }\n            else{\n                setCounter(counter+1);\n                \n            }\n        }\n    }\n\nuseEffect(()=>{\n    const value =showperPage*counter;\n\n    onPaginationChanges(value-showperPage,value);\n\n},[counter])\n  return (\n    <>\n    \n    <div className=' d-flex justify-content-center w-100'>\n      {counter>1?<button className='btn btn-outline-primary px-5' onClick={()=>{onButtonClick('prev')}}>Back</button>:null}\n    <label className=\"mx-5 my-2 \">{counter}</label>\n    {total>4&&(Math.ceil(total/11))!==counter?<button className='btn btn-outline-primary px-5' onClick={()=>{onButtonClick('next')}}>Next</button>:null}\n    </div>\n    </>\n\n   \n    \n  )\n\n}\n\nexport default Pagination","/home/chicmic/CricketApp/src/Components/Cells/addTeamForm/index.js",["239","240","241"],"import CreateRadio from \"Components/Atoms/customRadio\";\nimport { InputField } from \"Components/Atoms/customInput\";\nimport React, { useState } from \"react\";\nimport Select from \"react-select\";\n\n\n\nfunction TeamForm({ teamData, setTeamData, players, allPlayers, setPlayers, captain, setCaptain  }) {\n  // const playerLoggedInData = useSelector((state) => state.loginReducer.token)[0];\n  // console.log(playerLoggedInData);\n  const [countryData, setCountryDate] = useState([]);\n\n  const handleInput = (data) => {\n    setTeamData({\n      ...teamData,\n      [data.target.name]: data.target.value\n    })\n  }\n\n  const handleChange = (e) => {\n    console.log(e.value)\n    setCaptain(e)\n    setTeamData({\n      ...teamData,\n      teamCaptain: e.value\n    })\n  };\n\n\n  const handlePlayers = (players) => {\n    // console.log(players);\n    if (players.length < 11) {\n      setPlayers(players || []);\n      setTeamData({\n        ...teamData,\n        teamPlayers: players\n      })\n    }\n    else if (players.length > 10) {\n      return;\n    }\n\n  }\n  const handleRadio = (event) => {\n    console.log(\"event\", event)\n    setTeamData({\n      ...teamData,\n      teamType: event\n    })\n\n  }\n\n  const filterOption = (option, inputValue) => {\n    const { label, value } = option;\n    // looking if other options with same label are matching inputValue\n    const otherKey = option.filter(\n      opt => opt.label === label && opt.value.includes(inputValue)\n    );\n    return value.includes(inputValue) || otherKey.length > 0;\n  };\n\n  return (\n    <div>\n      <form>\n        <InputField\n          name=\"teamName\"\n          type=\"text\"\n          label=\"Team Name\"\n          placeholder=\"Enter the Team Name\"\n          value={teamData[\"teamName\"]}\n          onChange={handleInput}\n        />\n        <br />\n        <label><b>Select the type of team</b></label><br />\n        <CreateRadio values={[\"ipl\", \"international\"]} name={\"Type of team\"} state={teamData.teamType} setState={handleRadio} />\n\n        <label><b>Select the Players</b></label>\n\n        <Select options={allPlayers.map(val => ({ label: val.Name, value: val.Name, key: val.key}))} onChange={handlePlayers} name=\"teamPlayers\" value={players} closeMenuOnSelect={false} isMulti />\n        {players.length > 9 ? <div className=\"text-danger\">11 players selected</div> : null}\n        <label><b>Select Captain for the team</b></label>\n        <Select options={players.map(val => ({ label: val.label, value: val.value }))} onChange={handleChange} name=\"teamCaptain\" value={captain} />\n      </form>\n    </div>\n  );\n}\n\nexport default TeamForm;\n","/home/chicmic/CricketApp/src/Components/Atoms/customModal/index.js",["242"],"import React, { useState } from 'react'\nimport Button from 'react-bootstrap/Button';\nimport Modal from 'react-bootstrap/Modal';\n\nfunction CustomModal(props) {\n    const handleClose = () =>{\n        props.setShowModal(false);\n    }\n\n    // const handleSubmit =()=>{\n    //     console.log('Modal submitted');\n    //     props.setShowModal(false);\n    //   }\n  \n    return (\n      <>\n\n        <Modal\n          show={props.showModal}\n          onHide={handleClose}\n          backdrop=\"static\"\n          keyboard={false}\n        >\n          {props.header?<Modal.Header closeButton>\n            <Modal.Title>{props.title}</Modal.Title>\n          </Modal.Header>:null}\n          <Modal.Body>\n            {props.children}\n          </Modal.Body>\n         {props.footer? <Modal.Footer>\n            <Button variant=\"secondary\" onClick={handleClose}>\n              Close\n            </Button>\n            <Button variant=\"primary\" onClick={props.onSubmitModal}>Submit</Button>\n          </Modal.Footer>:null}\n        </Modal>\n      </>\n    );\n}\n\nexport default CustomModal\n","/home/chicmic/CricketApp/src/Components/Atoms/customRadio/index.js",[],"/home/chicmic/CricketApp/src/Components/Atoms/customInput/index.js",["243"],"import React, { useState } from \"react\";\n\n export const InputField = (props) => {\n  \n  return (\n    <div className=\"form-group\">\n      {props.label && <label htmlFor=\"input-field\">{props.label}</label>}\n      <input\n        type={props.type}\n        name={props.name}\n        value={props.value}\n        className=\"form-control w-77\"\n        placeholder={props.placeholder}\n        onChange={props.onChange}\n      />\n    </div>\n  );\n};\n\n","/home/chicmic/CricketApp/src/Views/AuthMessage/index.js",[],"/home/chicmic/CricketApp/src/Components/Atoms/TableButton/index.js",[],"/home/chicmic/CricketApp/src/Redux/Actions/updateTeamActions/index.js",[],"/home/chicmic/CricketApp/src/Redux/Actions/updateTeamActions/actionStates.js",[],{"ruleId":"244","replacedBy":"245"},{"ruleId":"246","replacedBy":"247"},{"ruleId":"248","severity":1,"message":"249","line":2,"column":10,"nodeType":"250"